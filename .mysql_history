CREATE TABLE Users
(
ulDnum INTEGER,
fName VARCHAR(30),
lName VARCHAR(30),
email VARCHAR(20),
Orginization VARCHAR(30),
Bio TEXT,
job_history TEXT,
Education VARCHAR(30),
profile_picture VARCHAR(50),
volunteer_work TEXT,
coding_languages VARCHAR(60),
PRIMARY KEY ulDnum
);
CREATE TABLE Users ( ulDnum INTEGER, fName VARCHAR(30), lName VARCHAR(30), email VARCHAR(20), Orginization VARCHAR(30), Bio TEXT, job_history TEXT, Education VARCHAR(30), profile_picture VARCHAR(50), volunteer_work TEXT, coding_languages VARCHAR(60), PRIMARY KEY ulDnum );
